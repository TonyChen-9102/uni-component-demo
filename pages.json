{
	"easycom":{
		"autoscan": true,
		"custom":{
			 // 匹配myComponents目录下mb-开头的组件
			"^mb-(.*)": "@/myComponents/mb-$1/mb-$1.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "index"
			}
		},
		{
			"path": "pages/componentDemo/ComponentDemo1",
			"style": {
				"navigationBarTitleText": "局部引用"
			}
		},
		{
			"path": "pages/componentDemo/ComponentDemo2",
			"style": {
				"navigationBarTitleText": "普通全局引用"
			}
		},
		{
			"path": "pages/componentDemo/ComponentDemo3",
			"style": {
				"navigationBarTitleText": "全局引用-easycom"
			}
		},
		{
			"path": "pages/componentDemo/ComponentDemo4",
			"style": {
				"navigationBarTitleText": "全局引用-插件"
			}
		}
	    ,{
            "path" : "pages/componentData/DataDemo1",
            "style" : {
				"navigationBarTitleText": "选项-data"
			}
        },{
            "path" : "pages/componentData/DataDemo2",
            "style" : {
				"navigationBarTitleText": "选项-props"
			}
        },{
            "path" : "pages/componentData/DataDemo3",
            "style" : {
				"navigationBarTitleText": "选项-computed"
			}
        },{
            "path" : "pages/componentData/DataDemo4",
            "style" : {
				"navigationBarTitleText": "选项-methods"
			}
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
